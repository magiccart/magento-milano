<config>
    <tabs>
        <magiccart translate="label">
            <label>Magiccart</label>
            <sort_order>100</sort_order>
        </magiccart>
    </tabs>
    <sections>
        <magicbrand translate="label" module="magicbrand">
            <label>Magic Brand</label>
            <tab>magiccart</tab>
            <sort_order>170</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <general translate="label">
                    <label>Magic Brand</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>1</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <fields>
                        <enabled translate="label comment">
                            <label>Enabled: </label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>0</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </enabled>
                        <jquery translate="label comment">
                            <label>Add jQuery: </label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>10</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </jquery>
                        <title translate="label comment">
                            <label>Title Brand: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>15</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </title>
                        <brand translate="label comment">
                            <label>Attribute Brand: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>20</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </brand>
                        <heading_slide translate="label">
                            <label>Setting Display Slider</label>
                            <frontend_model>adminhtml/system_config_form_field_heading</frontend_model>
                            <sort_order>25</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </heading_slide>
<!--                         <slide translate="label comment">
                            <label>Brand slide: </label>
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>30</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </slide> -->
                        <animationSpeed translate="label">
                            <label>Animation Speed: </label>
                            <!-- <depends><slide>1</slide></depends> -->
                            <frontend_type>text</frontend_type>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <sort_order>35</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_store>1</show_in_store>
                            <show_in_website>1</show_in_website>
                        </animationSpeed>
                       <autoPlay translate="label comment">
                            <label>Auto Play: </label>
                            <!-- <depends><slide>1</slide></depends> -->
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>40</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </autoPlay>
                        <autoPlaySpeed translate="label">
                            <label>Auto Play Speed: </label>
                            <!-- <depends><slide>1</slide></depends> -->
                            <frontend_type>text</frontend_type>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <sort_order>45</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_store>1</show_in_store>
                            <show_in_website>1</show_in_website>
                        </autoPlaySpeed>
                        <clone translate="label comment">
                            <label>Carousel Slider: </label>
                            <!-- <depends><slide>1</slide></depends> -->
                            <frontend_type>select</frontend_type>
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>50</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </clone>
                        <enableResponsiveBreakpoints translate="label comment">
                            <label>Enable Responsive Breakpoints: </label>
                            <frontend_type>select</frontend_type>
                            <!-- <depends><slide>1</slide></depends> -->
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>55</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </enableResponsiveBreakpoints>
                        <pauseOnHover translate="label comment">
                            <label>Pause On Hover: </label>
                            <frontend_type>select</frontend_type>
                            <!-- <depends><slide>1</slide></depends> -->
                            <source_model>adminhtml/system_config_source_yesno</source_model>
                            <sort_order>60</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>                            
                        </pauseOnHover>
                        <heading_brand translate="label">
                            <label>Setting Display Brands</label>
                            <frontend_model>adminhtml/system_config_form_field_heading</frontend_model>
                            <sort_order>65</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </heading_brand>
                        <portrait translate="label">
                            <label>Display in Screen 480: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>70</sort_order>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Number product show in screen 480</comment>
                        </portrait>
                        <landscape translate="label">
                            <label>Display in Screen 640: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>75</sort_order>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Number product show in screen 640</comment>
                        </landscape>
                        <tablet translate="label">
                            <label>Display in Screen 768: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>80</sort_order>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Number product show in screen 768</comment>
                        </tablet>
                        <visibleItems translate="label">
                            <label>Display in Screen > 768: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>85</sort_order>
                            <validate>required-entry validate-greater-than-zero</validate>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <comment>Number product show in screen > 768</comment>
                        </visibleItems>
                        <heightImages translate="label">
                            <label>Height of Images: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>90</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </heightImages>  
                        <widthImages translate="label">
                            <label>Width of Images: </label>
                            <frontend_type>text</frontend_type>
                            <sort_order>95</sort_order>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                        </widthImages>   
                    </fields>                   
                </general> 
            </groups>
        </magicbrand>
    </sections>
</config>

